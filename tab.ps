%!PS
% TAB

/Title {
        dup length /len exch def
            /title exch def
        /Helvetica findfont 10 scalefont setfont
        20 770 moveto (T) show
        20 760 moveto (A) show
        20 750 moveto (B) show
        /Helvetica findfont 22 scalefont setfont
        35 755 moveto /title load show
} def


/Staff {
        /y_offset exch def
        .1 setlinewidth
        /gstaff 6 array def
        0 10 50 {
                dup
                /y exch /y_offset load add def
                10 idiv 5 exch sub gstaff exch /y load 5 sub put
                40 /y load moveto
                550 /y load lineto
        } for
        160 120 600 {
                dup
                /y load moveto
                /y load 50 sub lineto
        } for
        /x 45 def
        /y /y_offset load def
        stroke
} def


/TimeSignature {
        /onebeat exch 1 string cvs def
        /n_beats exch 1 string cvs def
        /Helvetica findfont 30 scalefont setfont
        20 /y load 25 add moveto  /onebeat load show
        20 /y load        moveto  /n_beats load show
        % 20 load 30 add def
} def


/Note {
        /string-fret_array exch def

        /Helvetica findfont 12 scalefont setfont
        0 2 /string-fret_array load length 1 sub {
                dup 1 add /string-fret_array load exch get /fret exch def
                /string-fret_array load exch get /gstring exch def
                1 1 6 {
                        /gstring load eq {
                                /x load gstaff /gstring load 1 sub get moveto
                                /fret load 2 string cvs show
                        } if
                } for
        } for
        /x /x load 15 add def
} def


% Main
% Some common chords
/C-chord [ 5 3 4 2 3 0 2 1 1 0 ] def
/F-chord [ 6 1 5 3 4 3 3 2 2 1 1 1 ] def
/G-chord [ 6 3 5 2 4 0 3 0 2 0 1 3 ] def
% -=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-

(Yakety Ax) Title
650 Staff
4 4 TimeSignature

% 0 1 1 {
%       newpath
%       650 -100 50 { Staff } for
%       showpage
% } for

/G-chord load Note
/C-chord load Note
/F-chord load Note
/C-chord load Note
/G-chord load Note
/C-chord load Note
/F-chord load Note
/C-chord load Note

/G-chord load Note
/C-chord load Note
/F-chord load Note
/C-chord load Note
/G-chord load Note
/C-chord load Note
/F-chord load Note
/C-chord load Note

/G-chord load Note
/C-chord load Note

550 Staff
/G-chord load Note
/C-chord load Note
/F-chord load Note
/C-chord load Note

showpage
quit
